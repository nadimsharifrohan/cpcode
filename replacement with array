#include <bits/stdc++.h>

using namespace std;

int main() {
    int n;
    int arr[100];
    cin >> n;
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }

    for (int i = 0; i < n; i++) {
        if (arr[i] > 0) {
            cout << "2" << endl;
        }
        else if (arr[i] < 0) {
            cout << "1" << endl;
        }
    }

    return 0;
}
